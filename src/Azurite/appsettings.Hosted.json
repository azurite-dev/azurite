{
    "Logging": {
        "LogLevel": {
          "Default": "Information",
          "Microsoft": "Warning",
          "Microsoft.Hosting.Lifetime": "Information"
        }
      },
      "RateLimiting": {
        "RealIpHeader": "CF-Connecting-IP",
        "GeneralRules": [
          {
            "Endpoint": "*:/api/*",
            "Period": "10s",
            "Limit": 2
          },
          {
            "Endpoint": "*:/api/*",
            "Period": "15m",
            "Limit": 100
          },
          {
            "Endpoint": "*:/api/*",
            "Period": "12h",
            "Limit": 1000
          },
          {
            "Endpoint": "*:/api/*",
            "Period": "7d",
            "Limit": 10000
          }
        ]
      }
}